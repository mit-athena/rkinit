all:: rkinit_err.h

rkinit_err.h: 
	cd ../lib; make ../include/rkinit_err.h

depend:: rkinit_err.h

create_dir($(ATHINCDIR))
install_file(rkinit.h,$(ATHINCDIR)/rkinit.h)
install_file(rkinit_err.h,$(ATHINCDIR)/rkinit_err.h)

clean::
	rm -f *~ rkinit_err.h
