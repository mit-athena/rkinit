INSTALLDIR 	= /usr/include

all:: rkinit_err.h

rkinit_err.h: 
	cd ../lib; make ../include/rkinit_err.h

depend:: rkinit_err.h

install_file(rkinit.h,$(INSTALLDIR)/rkinit.h)
install_file(rkinit_err.h,$(INSTALLDIR)/rkinit_err.h)

clean::
	rm -f *~ rkinit_err.h
