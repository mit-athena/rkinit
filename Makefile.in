# $Id: Makefile.in,v 1.1 1999-10-05 17:08:32 danw Exp $

SHELL=/bin/sh
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
VPATH=@srcdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@ ${WARN_CFLAGS}
DEFS=@DEFS@
LDFLAGS=@LDFLAGS@
KRB4_LIBS=@KRB4_LIBS@
LIBS=@LIBS@
ALL_LIBS=-lrkinit ${KRB4_LIBS} ${LIBS}
ALL_CFLAGS=-I. ${CPPFLAGS} ${CFLAGS} ${DEFS}

all: rkinit

rkinit: rkinit.o
	${CC} ${LDFLAGS} -o rkinit rkinit.o ${ALL_LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	${INSTALL_PROGRAM} rkinit ${DESTDIR}${bindir}
	${INSTALL} -m 444 ${srcdir}/rkinit.1 ${DESTDIR}${mandir}/man1

clean:
	rm -f rkinit.o rkinit

distclean: clean
	rm -f config.cache config.log config.status Makefile
