# Makefile for rkinit manual pages
#  

DESTDIR		=
INSTALLDIR	= /usr/man

all:

depend:

install:
	cp rkinit.1 $(DESTDIR)$(INSTALLDIR)/man1
	cp rkinitd.8 $(DESTDIR)$(INSTALLDIR)/man8
	cp rkinit.3 $(DESTDIR)$(INSTALLDIR)/man3

clean:
	rm -f *~
